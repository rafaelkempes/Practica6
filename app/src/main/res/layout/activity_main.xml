<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Tmomil2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/chat"
        android:layout_width="60dp"
        android:layout_height="59dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/ic_sms"
        tools:layout_editor_absoluteY="328dp" />

    <ImageView
        android:id="@+id/chat2"
        android:layout_width="60dp"
        android:layout_height="59dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/ic_sms"
        tools:layout_editor_absoluteY="405dp"
        app:tint="#3BA13F" />

    <ImageView
        android:id="@+id/ImagenAndroid"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/background_1" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/BotonEstrella"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="360dp"
        android:backgroundTint="#FF3DD5"
        android:clickable="true"
        android:src="@drawable/ic_star"
        app:tint="#F6F3F3"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="20dp"
        tools:layout_editor_absoluteY="218dp"
        android:contentDescription="yeah"/>

    <ImageView
        android:id="@+id/ImagenTelefono"
        android:layout_width="67dp"
        android:layout_height="75dp"
        android:layout_marginStart="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_phone"
        tools:layout_editor_absoluteY="313dp" />

    <ImageView
        android:id="@+id/Chat1"
        android:layout_width="60dp"
        android:layout_height="59dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/ic_sms"
        tools:layout_editor_absoluteY="328dp"
        app:tint="#3BA13F"/>

    <TextView
        android:id="@+id/Movil1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="687100200"
        app:layout_constraintEnd_toStartOf="@+id/Chat1"
        app:layout_constraintHorizontal_bias="0.174"
        app:layout_constraintStart_toEndOf="@+id/ImagenTelefono"
        tools:layout_editor_absoluteY="328dp" />

    <TextView
        android:id="@+id/TMovil1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="195dp"
        android:text="Movil"
        app:layout_constraintEnd_toStartOf="@+id/Chat1"
        app:layout_constraintStart_toEndOf="@+id/ImagenTelefono"
        app:layout_constraintTop_toBottomOf="@+id/Movil1" />

    <TextView
        android:id="@+id/movil2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="157dp"
        android:text="699500600"
        app:layout_constraintEnd_toStartOf="@+id/chat2"
        app:layout_constraintTop_toBottomOf="@+id/TMovil1" />

    <TextView
        android:id="@+id/TextMovil2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="194dp"
        android:text="Movil"
        app:layout_constraintEnd_toStartOf="@+id/chat2"
        app:layout_constraintTop_toBottomOf="@+id/movil2" />

    <View
        android:id="@+id/divider"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="44dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintTop_toTopOf="@id/TextMovil2"
        tools:layout_editor_absoluteX="0dp"
        android:layout_marginRight="20dp"
        android:layout_marginLeft="100dp"/>

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="65dp"
        android:layout_height="59dp"
        app:srcCompat="@drawable/ic_email"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="528dp" />

    <TextView
        android:id="@+id/email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="41dp"
        android:text="info@gmail.com"
        app:layout_constraintStart_toEndOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/divider" />

    <TextView
        android:id="@+id/mail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="12dp"
        android:text="Mail"
        app:layout_constraintStart_toEndOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/email" />


</androidx.constraintlayout.widget.ConstraintLayout>